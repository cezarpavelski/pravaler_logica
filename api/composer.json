{
    "name": "teste_logica",
    "description": "Teste de Logica",
    "keywords": ["framework", "test"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "vendor/phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
